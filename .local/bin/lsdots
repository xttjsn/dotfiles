#!/bin/bash

USAGE="lsdots"

# Hi there!

if [[ $# -ne 0 ]]; then
	echo $USAGE
	exit
fi

declare -a DOTS=("$HOME/.emacs.d/el"
	  "$HOME/.emacs.d/snippets"
	  "$HOME/.emacs.d/workgroups"
	  "$HOME/.emacs.d/init.el"
	  "$HOME/.emacs.d/smex-items"
	  "$HOME/.urxvt"
	  "$HOME/.Xresources"
	  "$HOME/.bashrc"
	  "$HOME/.fishrc"
	  "$HOME/.profile"
	  "$HOME/.xinitrc"
	  "$HOME/.z"
	  "$HOME/.zshrc"
	  "$HOME/.local/bin")

for f in "${DOTS[@]}"
do
	find $f -type f
done
